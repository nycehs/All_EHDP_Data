        function SwitchTabContain() {
            hideHeader();
            // $("#secsubtopicMoreinfolink").hide();
            $("#dvtabcontainer").show();
            $("#dvtabcontainer2").show();
            $("#dvindicatorr").hide();
            $("#TopicBred").show();
            $("#RelTopicBred").hide();
            var name = $("#TopicName").val();
            $("#brdTopicName").html(name);
            CommanForPreviouseAndCurrentTab();
            $('#tabShowHide').val("0");
            $("#PriviousPageSub").val("0");

            ///hide the previous hovers
            $(".divMoreInfoPrint1").hide();
            $("#IndHowCalulated").hide();
            if ($("#TopicName").val() == "Health, Behavior, and Population" || $("#TopicName").val() == "Health") {
                $('[id^="MoreInfoDataHealth"]').hide();
                var subtopicid = $("#MoreInfoDataHealth-" + $("#subtopicId").val()).show();
                var obj = $(subtopicid).parent().parent().find('span')[0]
                DoLinkMouseOverHealth(obj)
            }
            else {
                $('[id^="MoreInfoData"]').hide();
                var subtopicid = $("#MoreInfoData-" + $("#subtopicId").val()).show();
                var obj = $(subtopicid).parent().parent().find('span')[0]
                DoLinkMouseOver(obj)
            }

        }
