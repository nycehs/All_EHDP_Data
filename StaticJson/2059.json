            hideHeader();
            $("#dvtabcontainer").show();
            $("#dvtabcontainer2").hide();
            $("#imghealth").show();
            $("#imghealthhover").hide();
            $("#imgenvironment").show();
            $("#imgenvironmenthover").hide();
