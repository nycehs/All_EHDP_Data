            $('#imgnew').mouseenter(function () {
                $('#imgnew').hide()
                $('#imgnewHover').show();
            });

            $('#imgnewHover').mouseleave(function () {
                $('#imgnew').show()
                $('#imgnewHover').hide();
            });
        }
        function TabFuction() {
            $('.nav-pills> li > a').bind("click", function () {
                $(this).tab('show');
                $('[id^="MoreInfoData"]').hide();
                if ($(this).attr("href") == "#Health") {
                    $("#dvtabcontainer2").show();
                    $("#imghealth").hide();
                    $("#imghealthhover").show();
                    $("#imgenvironment").show();
                    $("#imgenvironmenthover").hide();
                    $("#TopicName").val("Health, Behavior, and Population");
                    $("#ColorCode").val("4466a0");
                    $("#PriviousPageSub").val("0");

                }
                if ($(this).attr("href") == "#Behavior") {
                    window.location.href = "QuickView.aspx";
                    $("#TopicName").val("Behavior");
                    $("#ColorCode").val("5a5f84");
                    $("#PriviousPageSub").val("0");
                }
                if ($(this).attr("href") == "#Sustainability") {
                    window.location.href = "BuildATable.aspx";
                    $("#TopicName").val("Sustainability");
                    $("#ColorCode").val("729c88");
                    $("#PriviousPageSub").val("0");
                }
                if ($(this).attr("href") == "#Environment") {
                    $("#dvtabcontainer2").show();
                    $("#imgenvironment").hide();
                    $("#imghealth").show();
                    $("#imgenvironmenthover").show();
                    $("#imghealthhover").hide();
                    $("#TopicName").val("Environment");
                    $("#ColorCode").val("719b87");
                    $("#PriviousPageSub").val("0");
                }
                $("span[class='subunderline']").css('color', 'black');
            });
        }

        function TileClick(id) {
            $('#YourSelectedElement').val(id);
            $("#divButton").css("display", "block");
            return true;
        }

        ///Environment 
        function DoLinkMouseOver(obj) {
            $("span[class='subunderline']").css('color', 'black');
            $(obj).children().find("span[class='subunderline']").css('color', 'red');
            var divobject = obj.id;
            var res = divobject.split("-");
            var subtopicIdforhover = $("#" + divobject + " #tileimage" + res[1]).children("span").attr('id');
            $('[id^="MoreInfoData"]').hide();
            $("#MoreInfoData-" + subtopicIdforhover).show();
        }

        function DoLinkMouseOut(obj1) {
            $(obj1).children().find("span[class='subunderline']").css('color', 'black');
            if (obj1.style.background != 'white') {
                var id = $('#YourSelectedElement').val();
                if ((obj1.id) != "divSubtopic-" + id) {
                    var divobject1 = obj1.id;
                    var res1 = divobject1.split("-");
                    var color = $("#ColorCode").val();
                }
                $("#IndHowCalulated").hide();
                $("#subtopicDis1").hide();
                $("#IndHowCalulated").hide();
            }
        }

        ///Health
        function DoLinkMouseOverHealth(obj) {
            $("span[class='subunderline']").css('color', 'black');
            $(obj).children().find("span[class='subunderline']").css('color', 'red');
            var divobject = obj.id;
            var res = divobject.split("-");
            var subtopicIdforhover = $("#" + divobject + " #tileimage" + res[1]).children("span").attr('id');
            $('[id^="MoreInfoDataHealth"]').hide();
            $("#MoreInfoDataHealth-" + subtopicIdforhover).show();
        }

        function DoLinkMouseOutHealth(obj1) {
            $(obj1).children().find("span[class='subunderline']").css('color', 'black');
            if (obj1.style.background != 'white') {
                var id = $('#YourSelectedElement').val();
                if ((obj1.id) != "divSubtopic-" + id) {
                    var divobject1 = obj1.id;
                    var res1 = divobject1.split("-");
                    var color = $("#ColorCode").val();
                }
                $("#IndHowCalulated").hide();
                $("#subtopicDis1").hide();
                $("#IndHowCalulated").hide();
            }
        }

        function ShowSubtopicInfo(val) {
            $.ajax({
                type: "POST",
                url: "PublicTracking.aspx/ShowSubtopicInfo2",
                data: "{'SubtopicID': " + val + "}",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (msg) {
                    if (msg.d != '') {
                        $("#IndHowCalulated").hide();
                        $("#subtopicDis1").hide();
                        $("#subtopicDis1").html(msg.d);
                        $("#subtopicDis1").append($("<br/><a href='Glossary.aspx' target='_blank' style='color:#adadad; text-decoration: underline;font-size: 12px;'>GLOSSARY</a>"));
                    }
                },
                error: function () {
                }
            });

            $("#subtopicId").val(val);
        }
        function pageLoad() {
            HeaderImagesHideAndShow();
            TabFuction();
            SetColorForBread();
            LoadData();
            $("#textSearch").autocomplete({
                minLength: 1,
                select: function (event, ui) { doSearch(ui.item.address); alert(address); },
                open: function () { $(this).removeClass("ui-corner-all").addClass("ui-corner-top"); },
                close: function () { $(this).removeClass("ui-corner-top").addClass("ui-corner-all"); },
                minLength: 2,
                source: function (request, response) {
                    $.ajax({
                        url: "CustomErrors.aspx/GetCompletionListAll",
                        data: "{ 'prefixText': '" + request.term + "', 'count':50,'contextKey':'' }",
                        dataType: "json",
                        type: "POST",
                        contentType: "application/json; charset=utf-8",
                        success: function (data) {
                            response($.map(data.d, function (item) {
                                return {

                                    value: $.parseHTML($.parseJSON(item).First)[0].data,
                                    address: $.parseHTML($.parseJSON(item).Second)[0].data
                                }
                            }))
                        }
                    });
                } //source

            });
            $("#textSearch").keyup(function (e) {
                if (e.which === 13) {
                    $(".ui-menu-item").show();
                }
            }).menu.active;
        }
        function Google(urls) {
            ga('send', 'pageview', {
                'page': '' + urls,
                'title': 'PublicTracking'
            });

        }
        function doSearch(address) {

            var route = window.location.href;
            var routeData = route.split("?")[0];
            window.location.href = routeData + "?" + address;
        }
        function LoadData() {
            if ($('#tabShowHide').val() == 1) {
                $("#dvtabcontainer").hide();
                $("#dvtabcontainer2").hide();
                $("#dvindicatorr").show();
                $("#TopicBred").hide();
                $("#RelTopicBred").show();
                ShowHeader();
                var name = $("#TopicName").val();
                var subtopicName = $("#tabSubTopicName").html();
                $("#brdTopicName").html(name);
                $("#brdSubtopicName").html(subtopicName);
                $("#icon" + name).show();
            }
            else {
                $("#dvtabcontainer").show();
                $("#dvtabcontainer2").show();
                $("#dvindicatorr").hide();
                $("#TopicBred").show();
                $("#RelTopicBred").hide();
                $('[id^="MoreInfoData"]').hide();
                if ($("#TopicName").val() == "Health, Behavior, and Population" || $("#TopicName").val() == "Health") {
                    var subtopicid = $("#MoreInfoDataHealth-" + $("#subtopicId").val()).show();
                    var obj = $(subtopicid).parent().parent().find('span')[0]
                    if (typeof (obj) != 'undefined') {
                        DoLinkMouseOverHealth(obj)
                    }
                }
                else {
                    var subtopicid = $("#MoreInfoData-" + $("#subtopicId").val()).show();
                    var obj = $(subtopicid).parent().parent().find('span')[0]
                    if (typeof (obj) != 'undefined') {
                        DoLinkMouseOver(obj)
                    }
                }

            }

            CommanForPreviouseAndCurrentTab();
        }
        $(document).ready(function () {
            var hidwel = $("#hidwelcome").val();
            if (hidwel == "0") {
                $("#hidwelcome").val("1");
            }
        });

        function SetColorForBread() {
            var color = "#" + $("#ColorCode").val();
            $("#iconImageSubtopic").css('color', color);
            $("#iconImageSubtopicRead").css('color', color);
            $(".IndicatorLinks").css('color', color);
            if (color === '#719b87') {
                $("#brdTopicName").css('color', '#b2c1b7');
                $("#iconTopicName").css('background-color', '#b2c1b7');
                $("#iconTopicName").removeClass('bc_icon_health');
                $("#iconTopicName").addClass('bc_icon_Env');
            }
            else {
                $("#brdTopicName").css('color', '#8da2b5');
                $("#iconTopicName").css('background-color', '#8da2b5');
                $("#iconTopicName").removeClass('bc_icon_Env');
                $("#iconTopicName").addClass('bc_icon_health');
            }
            $(".bc_icon").css('background', color);
            $("#brdSubtopicName").css('color', color);
        }
        function CommanForPreviouseAndCurrentTab() {
            $('#Health').removeClass('active');
            $('#tabHealth').removeClass('active');
            $('#tabEnvironment').removeClass('active');
            $('#Environment').removeClass('active');

            if ($("#PriviousPageSub").val() == "1" || $('#tabShowHide').val() == "1") {
                SetColorForBread();
                if ($("#TopicName").val() == "Health, Behavior, and Population" || $("#TopicName").val() == "Health") {
                    $('#tabHealth').attr('class', 'active');
                    $("#tabHealth").tab('show');
                    $('#Health').addClass('active');
                    $("#imghealth").hide();
                    $("#imghealthhover").show();
                    var intenalid = $("#InternalId").val();
                    var text = $("#" + intenalid).text();
                    var obj = {
                        id: "" + intenalid,
                        text: "" + text

                    };
                    if ($('#tabShowHide').val() == "1" && $("#PriviousPageSub").val() == "1") {
                        DoHowCalculated(obj);
                        $('#tabShowHide').val("0")
                    }
                }
                if ($("#TopicName").val() == "Environment") {
                    $('#tabEnvironment').attr('class', 'active');
                    $("#tabEnvironment").tab('show');
                    $('#Environment').addClass('active');
                    $("#imgenvironment").hide();
                    $("#imgenvironmenthover").show();
                }

            }
        }
