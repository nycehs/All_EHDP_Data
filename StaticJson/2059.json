        function GetSubtopicInfoByNameOut(obj) {
            $("#IndHowCalulated").hide();
        }
        function HeaderImagesHideAndShow() {
            $('#imgNeighborhoodReport').mouseenter(function () {
                $('#imgNeighborhoodReport').hide()
                $('#imgNeighborhoodReportHover').show();
            });
